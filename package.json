{
  "name": "react-luau-props-helper",
  "displayName": "React Luau Props Helper",
  "description": "Customizable prop autocomplete for React-Luau createElement calls.",
  "version": "1.0.0",
  "publisher": "leifstout",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leifstout/react-luau-props-helper.git"
